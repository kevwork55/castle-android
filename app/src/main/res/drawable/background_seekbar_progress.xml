<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/progressshape">
        <clip>
            <shape>
                <corners android:radius="@dimen/seekbar_track_corner_radius" />
                <gradient
                    android:angle="180"
                    android:endColor="@color/main_end_gradient"
                    android:startColor="@color/main_start_gradient"
                    android:type="linear" />
            </shape>

        </clip>
    </item>
</layer-list>